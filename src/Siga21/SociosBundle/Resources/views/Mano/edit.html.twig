{% extends "Siga21SociosBundle::basesocios.html.twig" %}
{% block title "Listado de Socios" %}
{% block cabeza %}
	<h1>Socios</h1>
{% endblock %}
{% block contenido %}
<br />
<form action="{{ path('mano_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit"><img src="{{ asset('bundles/Siga21/images/editar.gif') }}" alt="Editar"></button>
    </p>
</form>
<a href="{{ path('mano') }}"><img src="{{ asset('bundles/Siga21/images/back.gif') }}" alt="Volver"></a>
<form action="{{ path('mano_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
         <button type="submit"><img src="{{ asset('bundles/Siga21/images/trash.gif') }}" alt="Borrar"></button>
</form>   

{% endblock %}

{% block footer %}
<p>(c) nocopyright 2011</p>
{% endblock %}
