<h1>Tareas</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Id</th>
            <td>{{ entity.Id }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.Nombre }}</td>
        </tr>
        <tr>
            <th>Fecha</th>
            <td>{{ entity.Fecha|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Socio</th>
            <td>{{ entity.socios }}</td>
        </tr>
        <tr>
            <th>Notas</th>
            <td>{{ entity.Notas }}</td>
        </tr>
        <tr>
            <th>Terminada</th>
            <td>{{ entity.Terminada }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('tareas') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('tareas_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('tareas_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
